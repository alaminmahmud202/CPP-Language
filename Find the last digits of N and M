#include <iostream>
using namespace std;

int main() {
    long long N, M;
    cin >> N >> M;

    // Find the last digits of N and M
    int last_digit_N = N % 10;
    int last_digit_M = M % 10;

    // Calculate the summation of the last digits
    int result = last_digit_N + last_digit_M;

    // Print the result
    cout << result << endl;

    return 0;
}


//Processing:
//Last digit of 12345 = 5
//Last digit of 6789 = 9
//Sum of last digits: 5 + 9 = 14
//output:14
